<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/public_height_60dp"
        android:layout_marginStart="@dimen/public_mar_left_20dp"
        android:layout_marginEnd="@dimen/public_mar_right_20dp"
        android:gravity="center_vertical"
        android:text="@string/music_new_sheet"
        android:textSize="@dimen/public_font_20sp" />

    <EditText
        android:id="@+id/dialog_sheet_name_et"
        android:layout_width="match_parent"
        android:layout_height="@dimen/public_height_60dp"
        android:layout_marginStart="@dimen/public_mar_left_20dp"
        android:layout_marginEnd="@dimen/public_mar_right_20dp" />
</LinearLayout>